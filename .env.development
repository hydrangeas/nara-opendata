# 開発環境設定

NODE_ENV=development
LOG_LEVEL=debug

# 開発環境用の設定
USE_MOCK_DATA=false
ENABLE_HOT_RELOAD=true
DEBUG_MODE=true

# 開発環境用のCORS設定
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,http://localhost:4173

# 機能フラグ（開発環境では全機能有効）
FEATURE_API_DOCS_ENABLED=true
FEATURE_HEALTH_CHECK_ENABLED=true
FEATURE_METRICS_ENABLED=true