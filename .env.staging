# ステージング環境設定

NODE_ENV=staging
LOG_LEVEL=info

# ステージング環境用の設定
TEST_USERS=test1@example.com,test2@example.com
DEBUG_ENDPOINTS=true

# ステージング環境用のCORS設定
CORS_ORIGINS=https://staging.example.com,https://staging-api.example.com

# 機能フラグ（ステージング環境）
FEATURE_API_DOCS_ENABLED=true
FEATURE_HEALTH_CHECK_ENABLED=true
FEATURE_METRICS_ENABLED=true